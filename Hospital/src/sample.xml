<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
			

<beans>
   
   <bean id="h1" class="org.jsp.app.Hospital">
   <property name="hospitalName" value = "manaHospital" ></property>
   <property name="location" value ="hyd" ></property>
   <property name="doctor" ref = "d1" ></property>
   </bean>
   <bean id= "d1" class = "org.jsp.app.Doctor">
   <constructor-arg value = "sai"></constructor-arg>
   <constructor-arg value = "sex"></constructor-arg>
   </bean>

</beans>