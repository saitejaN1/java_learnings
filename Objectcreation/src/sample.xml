<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
			

<beans>
   
   <bean id="b1" class="org.jsp.app.Bus">
   <constructor-arg value = "3245" ></constructor-arg>
   <property name="busDriverName" value = "Raju"></property>
    <property name="details" ref = "d1" ></property>
   
   
   </bean>
   
   <bean id = "d1" class = "org.jsp.app.BusDetails">
   <property name="busSeats" value = "65"></property>
   <property name="startingCity" value = "Hyderabad"></property>
   <property name="endingCity" value = "Bangalore"></property>
   <property name="busColour" value = "Yellow"></property>
   
   
   </bean>

</beans>